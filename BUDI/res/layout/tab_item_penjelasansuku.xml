<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bottombatik"
    android:orientation="vertical" xmlns:android1="http://schemas.android.com/apk/res/android">

    <TextView
        android1:id="@+id/txtSuku"
        android1:layout_width="fill_parent"
        android1:layout_height="wrap_content"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/txtP"
        android1:layout_marginLeft="10dp"
        android1:layout_marginRight="10dp"
        android1:background="@drawable/border"
        android1:layout_marginTop="23dp"
        android1:text=""
        android1:textColor="#000"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/txtP"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/txtSuku"
        android1:layout_alignParentTop="true"
        android1:layout_marginTop="48dp"
        android1:background="@drawable/border"
        android1:text="Suku pada provinsi"
        android1:textAppearance="?android:attr/textAppearanceMedium"
        android1:textColor="#000" />

</RelativeLayout>